<%- include('partials/header') %>

<div class="container">
    <div class="row">
      <div class="col-md-6 m-auto">
        <h1 class="text-center display-4 my-4">Make New Course</h1>
        <form action="/courses" method="POST">
          <div class="form-group">
            <label>Course name</label>
            <input class="form-control" type="text" name="name" required>
          </div>
          <div class="form-group">
            <label>Course Image URL</label>
            <input class="form-control" type="url" name="image" id="img" required>
            <!-- <input type="file" name="image" id="img" accept="image/*" class="custom-file-input" required>
            <label for="img" class="custom-file-label">Choose Image</label> -->
          </div>
          <input type="submit" value="Submit" class="btn btn-outline-dark btn-block">
        </form>
      </div>
    </div>
  </div>




<%- include('partials/footer') %>